{
	"name": "Let's Get!",
	"author": "Noah Shanaberger",
	"description": "Download those pesky raw files on Github!",
	"manifest_version": 2,
	"version": "0.0.7",
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
	"content_scripts": [
		{
			"matches": ["*://github.com/*"],
			"js": ["content_script.js"]
		},
		{
			"matches": ["*://gist.github.com/*"],
			"js": ["gist_content_script.js"]
		}
	],
	"permissions": [
		"downloads",
		"storage",
		"tabs",
		"*://github.com/*",
		"*://gist.github.com/*"
	],
	"browser_action": {
		"default_popup": "index.html",
		"default_title": "Let's Get!",
		"default_icon": {
			"16": "icon_16.png",
			"32": "icon_32.png",
			"64": "icon_64.png"
		}
	},
	"browser_specific_settings": {
		"gecko": {
			"id": "{8129971f-42ae-4350-b191-b2ff190437c0}"
		}
	},
	"icons": {
		"16": "icon_16.png",
		"48": "icon_48.png",
		"128": "icon_128.png"
	}
}
