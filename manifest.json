{
	"name": "Let's Get!",
	"description": "Download those pesky raw files on Github!",
	"manifest_version": 2,
	"version": "0.0.2",
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
	"content_scripts": [
		{
			"matches": ["*://github.com/*"],
			"js": ["content_script.js"]
		}
	],
	"permissions": [
		"downloads",
		"*://github.com/*"
	]
}
